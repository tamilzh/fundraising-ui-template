{
  "version": 2,
  "env": {
      "REACT_APP_VERSION": "1.0"
    },
  "functions": {
    "env": {
      "REACT_APP_VERSION": "1.q"
    }
  },
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ]
}